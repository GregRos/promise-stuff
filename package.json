{
  "name": "promise-stuff",
  "version": "0.2.0",
  "description": "A library for extending existing Promise/A+ implementations with cool extra features that work well with async/await. With TypeScript support",
  "keywords": [
    "promise",
    "promise-a",
    "timeout",
    "finally",
    "typescript",
    "extension",
    "utility",
    "async"
  ],
  "directories": {
    "test": "dist/__test__",
    "lib": "dist/lib",
    "example": "src/examples"
  },
  "files": [
    "dist/",
    "src/"
  ],
  "repository": "https://github.com/GregRos/promise-stuff",
  "homepage": "https://github.com/GregRos/promise-stuff",
  "main": "dist/lib/index.js",
  "dependencies": {},
  "devDependencies": {
    "@types/del": "^3.0.0",
    "@types/gulp": "^4.0.5",
    "@types/gulp-sourcemaps": "0.0.32",
    "@types/node": "^8.5.2",
    "ava": "^0.24.0",
    "ava-spec": "^1.1.1",
    "merge2": "^1.2.0",
    "typescript": "^2.7.2"
  },
  "scripts": {
    "test": "ava"
  },
  "author": "GregRos",
  "license": "MIT",
  "ava": {
    "files": [
      "dist/__test__/**/*.spec.js",
      "!dist/__test/helpers/*.*"
    ],
    "source": [
      "src/**/*.ts"
    ]
  },
  "typings": "dist/lib/index.d.ts"
}
